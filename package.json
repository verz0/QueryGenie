{
  "name": "nlp2sql-react",
  "version": "2.0.0",
  "description": "NLP2SQL with React frontend and Node.js backend",
  "scripts": {
    "install:all": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm run dev",
    "start:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "test": "concurrently \"cd backend && npm test\" \"cd frontend && npm test\"",
    "clean": "rm -rf frontend/node_modules backend/node_modules frontend/build backend/uploads"
  },
  "keywords": [
    "nlp",
    "sql",
    "react",
    "node",
    "ai",
    "database"
  ],
  "author": "NLP2SQL Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "nodemon": "^3.1.10"
  }
}
